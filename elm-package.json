{
    "version": "3.0.0",
    "summary": "HTML helper functions to comply to Atlassian AUI css",
    "repository": "https://github.com/stil4m/elm-aui-css.git",
    "license": "BSD3",
    "source-directories": [
        "src",
        "examples"
    ],
    "exposed-modules": [
        "Aui.Avatars",
        "Aui.Backdrop",
        "Aui.Badges",
        "Aui.Buttons",
        "Aui.Dropdown",
